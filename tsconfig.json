{
  "compilerOptions": {
    "target": "ES6",
    "module": "Node16",
    "lib": ["ES6"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": false
  },
  "exclude": ["node_modules", "**/*.spec.ts", "build"]
}
